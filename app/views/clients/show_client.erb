<header>
  <form  action='/logout'>
    <input type="submit" value="Logout">
  </form>


</header>
<body>
  <h1> <%= @client.name %> </h1>
  <h3> Current Projects </h3>
  <%@client.projects.each |project| do  %>
  <ul>
    <li>project.name<li>
    </ul>
      <% end %>
  <form  action='/projects/new'>
    <input type="submit" value="Create a new project">
  </form>

  <form  action='/clients/<%= @client.id %>/edit'>
    <input type="submit" value="Edit Account Settings">
  </form>
</body>
